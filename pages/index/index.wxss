.screen-swiper{
    width: 95%;
    -webkit-box-shadow: 0 1px 3px 3px rgba(0,0,0,0.05) ;
    -moz-box-shadow: 0 1px 3px 3px rgba(0,0,0,0.05);
    box-shadow: 0 1px 3px 3px rgba(0,0,0,0.05) ;
    overflow: hidden;
    -webkit-border-radius: 20rpx;
    -moz-border-radius: 20rpx;
    border-radius: 20rpx;
    margin: 2.5%;
}
swiper-item{

    overflow: hidden;
    -webkit-border-radius: 20rpx;
    -moz-border-radius: 20rpx;
    border-radius: 20rpx;
}
.screen-swiper image{
    -webkit-border-radius: 20rpx;
    -moz-border-radius: 20rpx;
    border-radius: 20rpx;
}
