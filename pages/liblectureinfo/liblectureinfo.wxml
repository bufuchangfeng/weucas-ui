<!--pages/liblectureinfo/liblectureinfo.wxml-->
<view class="padding-xl radius shadow-warp bg-white margin-top" bindtap="showCardView" data-index="{{index}}">
          <view>讲座名称： {{liblecture.name}}</view>
          <view>讲座地点： {{liblecture.place}}</view>
          <view>主讲人:  {{liblecture.who}}</view>
          <view>开始时间： {{ liblecture.start_time }}</view>
          <view>结束时间： {{ liblecture.end_time }}</view>
          <view>讲座介绍： {{ liblecture.content }}</view>          
</view>

